            <declare name="valorJog2" type="Real" array="False" size=""/>
            <assign variable="valorJog2" expression="1000000 * percJog2"/>
            <output expression="&quot;O valor do pr&#234;mio do jogador 1 &#233;:&quot; &amp; valorJog1" newline="True"/>
            <output expression="&quot;O valor do pr&#234;mio do jogador 2 &#233;:&quot; &amp; valorJog2" newline="True"/>
