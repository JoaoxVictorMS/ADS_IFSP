    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;Informe os pontos do primeiro jogador:&quot;" newline="True"/>
            <declare name="ponto1" type="Real" array="False" size=""/>
            <input variable="ponto1"/>
